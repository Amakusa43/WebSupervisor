<script>
    $(document).ready(function () {
        $("#classnumber").change(function () {
            var week = $("#week").val();
            var day = $("#day").val();
            var classnumber = $(this).val();
            $.ajax({
                url: '/Home/ArrageAddwdc',
                type: 'post',
                contentType: 'application/json;charset=utf-8',
                data: JSON.stringify({
                    "week": week,
                    "day": day,
                    "classnumber": classnumber,
                }),
                success: function (teachernames) {
                    for (i in teachernames) {
                        $("#teachername").append('<option value="' + teachernames[i] + '">' + teachernames[i] + '</option>');  //添加一项option ;
                    }
                },
                error: function () {
                    alert("出错了");
                }
            })
        })
    });
</script>
